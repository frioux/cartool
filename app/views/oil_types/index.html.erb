<h1>Listing oil_types</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for oil_type in @oil_types %>
  <tr>
    <td><%=h oil_type.name %></td>
    <td><%= link_to 'Show', oil_type %></td>
    <td><%= link_to 'Edit', edit_oil_type_path(oil_type) %></td>
    <td><%= link_to 'Destroy', oil_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New oil_type', new_oil_type_path %>
